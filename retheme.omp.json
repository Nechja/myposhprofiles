{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "console_title_template": "{{if .Root}}[root] {{end}}{{.Shell}} in <{{.Folder}}>",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "template": "<#D52D00>\u250c</>",
          "background": "transparent",
          "foreground": "p:white",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "os",
          "style": "diamond",
          "template": " <#FFFFFF>{{.Icon}}</>{{if .WSL}} (WSL){{end}}\u2800",
          "properties": {
            "linux": "\uf17c",
            "macos": "\uf179",
            "ubuntu": "\uf31b",
            "windows": "\uf17a"
          },
          "background": "#FF9A56",
          "foreground": "p:black",
          "Text": "",
          "Duration": 0,
          "NameLength": 0,
          "trailing_diamond": "\ue0c6"
        },
        {
          "type": "shell",
          "style": "diamond",
          "template": " \uf489 {{.Name}}\u2800",
          "background": "p:white",
          "foreground": "p:black",
          "Text": "",
          "Duration": 0,
          "NameLength": 0,
          "trailing_diamond": "\ue0c6"
        },
        {
          "type": "session",
          "style": "diamond",
          "template": " \udb85\uddcf {{.HostName}}\ue0bd{{.UserName}}\u2800",
          "background": "#D162A4",
          "foreground": "p:black",
          "Text": "",
          "Duration": 0,
          "NameLength": 0,
          "trailing_diamond": "\ue0c6"
        },
        {
          "type": "git",
          "style": "diamond",
          "template": "{{.HEAD}}\u2800",
          "properties": {
            "branch_icon": " \ue0a0 "
          },
          "background": "#A30262",
          "foreground": "p:white",
          "Text": "",
          "Duration": 0,
          "NameLength": 0,
          "trailing_diamond": "\ue0c6"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "executiontime",
          "style": "diamond",
          "template": " {{.FormattedMs}} ",
          "properties": {
            "style": "austin",
            "threshold": 0
          },
          "background": "#A30262",
          "foreground": "transparent",
          "Text": "",
          "Duration": 0,
          "leading_diamond": "\ue0c7",
          "NameLength": 0
        },
        {
          "type": "time",
          "style": "diamond",
          "template": "<#A30262> \udb85\uddff </>{{.CurrentDate | date .Format}}",
          "properties": {
            "time_format": "02/01/06 15:04"
          },
          "background": "p:white",
          "foreground": "#A30262",
          "trailing_diamond": "\ue0c6",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "path",
          "style": "plain",
          "template": "<#D52D00>\u2514</><#B55690>[ </>{{.Path}}<#B55690> ]</>",
          "properties": {
            "folder_icon": "\uf07b",
            "folder_separator_template": "<#93d0ff> » </>",
            "home_icon": "\udb81\udfd2",
            "style": "agnoster"
          },
          "background": "p:red",
          "foreground": "p:white",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "status",
          "style": "plain",
          "foreground_templates": [
            "{{if gt .Code 0}}#e100ff{{end}}"
          ],
          "template": "❯",
          "properties": {
            "always_enabled": true
          },
          "background": "transparent",
          "foreground": "#B55690",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        }
      ],
      "newline": true
    }
  ],
  "palette": {
    "black": "#111111",
    "white": "#f7f7f7"
  }
}
