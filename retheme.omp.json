{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "console_title_template": "{{if .Root}}[root] {{end}}{{.Shell}} = <{{.Folder}}>",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "spotify",
          "style": "diamond",
          "powerline_symbol": "\ue0b0",
          "leading_diamond": "\ue0c7",
          "trailing_diamond": "\ue0c6",
          "template": " {{ .Icon }}{{ if ne .Status \"stopped\" }}{{ (abbrev 19 .Artist) }} - {{ .Track }}{{ end }}  ",
          "properties": {
            "paused_icon": "\uf8e3 ",
            "playing_icon": "\ue602 ",
            "stopped_icon": "\uf04d "
          },
          "background": "transparent",
          "foreground": "#929292",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "template": "<#D52D00>\u250c</>",
          "background": "transparent",
          "foreground": "p:white",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "os",
          "style": "diamond",
          "trailing_diamond": "\ue0c6",
          "template": " <#FFFFFF>{{.Icon}}</>{{if .WSL}} (WSL){{end}}\u2800",
          "properties": {
            "linux": "\uf17c",
            "macos": "\uf179",
            "mapped_shell_names": {
              "pwsh": "PS"
            },
            "ubuntu": "\uf31b",
            "windows": "\uf17a"
          },
          "background": "#FF9A56",
          "foreground": "p:black",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "shell",
          "style": "diamond",
          "trailing_diamond": "\ue0c6",
          "template": " \uf489 {{.Name}}\u2800",
          "background": "p:white",
          "foreground": "p:black",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "session",
          "style": "diamond",
          "trailing_diamond": "\ue0c6",
          "template": " \udb85\uddcf {{.HostName}}\ue0bd{{.UserName}}\u2800",
          "background": "#D162A4",
          "foreground": "p:black",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "git",
          "style": "diamond",
          "trailing_diamond": "\ue0c6",
          "template": "{{.HEAD}}\u2800",
          "properties": {
            "branch_icon": " \ue0a0 "
          },
          "background": "#A30262",
          "foreground": "p:white",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "executiontime",
          "style": "diamond",
          "leading_diamond": "\ue0c7",
          "trailing_diamond": "\ue0c6",
          "template": " {{.FormattedMs}} ",
          "properties": {
            "style": "austin",
            "threshold": 0
          },
          "background": "#A30262",
          "foreground": "transparent",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "sysinfo",
          "style": "diamond",
          "powerline_symbol": "\ue0b0",
          "trailing_diamond": "\ue0c6",
          "template": " \ue266 {{ round .PhysicalPercentUsed .Precision }}% {{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB ",
          "properties": {
            "precision": 2
          },
          "background": "#ffffff",
          "foreground": "#A30262",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "status",
          "style": "diamond",
          "background_templates": [
            "{{ if .Error }}#e91e63{{ end }}"
          ],
          "trailing_diamond": "\ue0c6",
          "template": " \ue23a ",
          "properties": {
            "always_enabled": true
          },
          "background": "#FF9A56",
          "foreground": "#ffffff",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "battery",
          "style": "diamond",
          "trailing_diamond": "\ue0c6",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}#D162A4{{end}}",
            "{{if eq \"Discharging\" .State.String}}#DD52D00{{end}}",
            "{{if eq \"Full\" .State.String}}#A30262{{end}}"
          ],
          "template": " {{ if not .Error }} {{ .Icon }}{{ end }} ",
          "properties": {
            "charged_icon": "\ue22f ",
            "charging_icon": "\ue234 ",
            "discharging_icon": "\ue231 "
          },
          "background": "#ffeb3b",
          "foreground": "#193549",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "path",
          "style": "plain",
          "template": "<#D52D00>\u2514</><#B55690>[ </>{{.Path}}<#B55690> ]",
          "properties": {
            "folder_icon": "\uf07b",
            "folder_separator_template": "<#93d0ff> » </>",
            "home_icon": "\udb81\udfd2",
            "style": "agnoster"
          },
          "background": "p:red",
          "foreground": "p:white",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "python",
          "style": "plain",
          "template": " <#FFDE57>\ue235 {{ if .Venv }}{{ .Venv }}{{ end }}<#B55690>",
          "properties": {
            "fetch_virtual_env": true
          },
          "background": "transparent",
          "foreground": "#FFDE57",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "node",
          "style": "plain",
          "template": " <#8CC84B>\ue718 {{.Version}}<#B55690>",
          "background": "transparent",
          "foreground": "#8CC84B",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "dotnet",
          "style": "plain",
          "template": " <#512BD4>\udb82\udeae {{ .Full }}<#B55690>",
          "background": "transparent",
          "foreground": "#512BD4",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "az",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " \uebd8 {{ .EnvironmentName }}",
          "properties": {
            "source": "pwsh"
          },
          "background": "#9ec3f0",
          "foreground": "#000000",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "rust",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " \ue7a8 {{ .Full }} ",
          "background": "#99908a",
          "foreground": "#193549",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "terraform",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": "{{.WorkspaceName}}",
          "background": "#ebcc34",
          "foreground": "#000000",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "pulumi",
          "style": "diamond",
          "powerline_symbol": "\ue0cf",
          "template": "{{ .Stack }}{{if .User }} :: {{ .User }}@{{ end }}{{ if .URL }}{{ .URL }}{{ end }}",
          "background": "#662d91",
          "foreground": "#ffffff",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "azd",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " \uebd8 {{ .DefaultEnvironment }} ",
          "background": "#9ec3f0",
          "foreground": "#000000",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "npm",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": "\ue71e {{ .Full }} ",
          "background": "#ffeb3b",
          "foreground": "#193549",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "root",
          "style": "plain",
          "template": "\uf0e7  ",
          "background": "transparent",
          "foreground": "#ffff66",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "status",
          "style": "plain",
          "foreground_templates": [
            "{{if gt .Code 0}}#e100ff{{end}}"
          ],
          "template": "❯",
          "properties": {
            "always_enabled": true
          },
          "background": "transparent",
          "foreground": "#B55690",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        }
      ],
      "newline": true
    }
  ],
  "palette": {
    "black": "#111111",
    "white": "#f7f7f7"
  }
}
